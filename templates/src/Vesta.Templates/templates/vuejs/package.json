{
  "name": "ProjectName-frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dev:mock": "vite --mode=development-mock",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest run --environment jsdom",
    "test:unit:coverage": "vitest run --coverage --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --max-warnings 0",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --max-warnings 0 --fix",
    "lint:css": "stylelint ./src/**/*.{vue,scss,css}",
    "lint:css:fix": "stylelint ./src/**/*.{vue,scss,css} --fix",
    "prepare": "husky install",
    "generate": "plop --plopfile ./plop/plopfile.mjs",
    "docker:generate-image": "docker build --pull --rm . -t ProjectName:latest"
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.0-alpha.41",
    "@vuelidate/validators": "^2.0.0-alpha.29",
    "pinia": "^2.0.14",
    "primeicons": "^5.0.0",
    "primevue": "^3.13.1",
    "vue": "^3.2.36",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.36",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.11.3",
    "cypress": "^10.0.2",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^8.0.1",
    "jsdom": "^19.0.0",
    "lint-staged": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "plop": "^3.1.0",
    "prettier": "^2.5.1",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.9.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "typescript": "~4.7.2",
    "vite": "^2.9.9",
    "vitest": "^0.13.0",
    "vue-tsc": "^0.35.2"
  }
}
